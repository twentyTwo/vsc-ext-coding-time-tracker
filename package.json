{
  "name": "simple-coding-time-tracker",
  "displayName": "Simple Coding Time Tracker",
  "description": "Track and visualize your coding time across projects",
  "version": "0.1.1",
  "publisher": "noorashuvo",
  "icon": "icon-sctt.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "simpleCodingTimeTracker.showSummary",
        "title": "Show Coding Time Summary"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/twentyTwo/vscode-time-tracker.git"
  }
}